{
    "name": "wsydney76/craft-members",
    "description": "Enables membership functionality",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "wsydney@web.de",
        "issues": "https://github.com/wsydney76/craft-members/issues?state=open",
        "source": "https://github.com/wsydney76/craft-members",
        "docs": "https://github.com/wsydney76/craft-members",
        "rss": "https://github.com/wsydney76/craft-members/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.6"
    },
    "require-dev": {
        "craftcms/ecs": null,
        "craftcms/phpstan": null
    },
    "autoload": {
        "psr-4": {
            "wsydney76\\members\\": "src/"
        }
    },
    "extra": {
        "handle": "members",
        "name": "Members",
        "developer": "wsydney76",
        "documentationUrl": "https://github.com/wsydney76/craft-members"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

